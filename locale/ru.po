msgid "WIKI_PAGE_NITROGEN-FIXING_PLASTID"
msgstr "Азотфиксирующая пластида"

msgid "WIKI_PAGE_NITROGENASE"
msgstr "Нитрогеназа"

msgid "WIKI_PAGE_NUCLEUS"
msgstr "Ядро"

msgid "WIKI_PAGE_ORGANELLES_ROOT"
msgstr "Органеллы"

msgid "WIKI_PAGE_OXYTOXISOME"
msgstr "Окситоксисома"

