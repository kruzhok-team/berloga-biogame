msgstr "Аксон"

msgid "WIKI_PAGE_BINDING_AGENT"
msgstr "Связующий агент"

msgid "WIKI_PAGE_BIOLUMINESCENT_VACUOLE"
msgstr "Биолюминесцентная вакуоль"

